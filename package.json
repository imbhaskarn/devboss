{
  "name": "api",
  "version": "1.0.0",
  "description": "A Node.js application with Prisma, ES6, Jest, and TypeScript",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "format": "prettier --write .",
    "db:generate": "prisma generate --schema src/.prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema src/.prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema src/.prisma/schema.prisma",
    "db:push": "prisma db push --schema src/.prisma/schema.prisma",
    "prisma:validate": "prisma db push --schema src/.prisma/schema.prisma",
    "test": "jest"
  },
  "keywords": [
    "node",
    "prisma",
    "jest",
    "typescript",
    "es6"
  ],
  "author": "Bhaskar Sharma",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.14",
    "@types/ejs": "^3.1.2",
    "@types/nodemailer": "^6.4.10",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.5",
    "prisma": "^5.2.0",
    "tsconfig-paths": "^4.2.0",
    "unique-username-generator": "^1.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/types": "^29.6.3",
    "@prisma/client": "5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.2",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.6.4",
    "jest": "^29.6.3",
    "joi-to-typescript": "^4.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.2"
  }
}
