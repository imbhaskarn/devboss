{
    "name": "api",
    "version": "1.0.0",
    "description": "A Node.js application with Prisma, ES6, Jest, and TypeScript",
    "main": "dist/src/app.js",
    "scripts": {
        "start": "node dist/src/app.js",
        "dev": "nodemon",
        "build": "tsc",
        "format": "prettier --write .",
        "migrate:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
        "db:push": "prisma db push --schema src/prisma/schema.prisma",
        "prisma:validate": "prisma db push --schema src/prisma/schema.prisma",
        "test": "jest"
    },
    "keywords": [
        "node",
        "prisma",
        "jest",
        "typescript",
        "es6"
    ],
    "author": "Bhaskar Sharma",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.1",
        "prisma": "^5.2.0"
    },
    "devDependencies": {
        "@jest/types": "^29.6.3",
        "@prisma/client": "5.2.0",
        "@types/bcrypt": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@babel/preset-env": "^7.22.15",
        "@babel/preset-typescript": "^7.22.15",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.5.2",
        "babel-jest": "^29.6.4",
        "jest": "^29.6.3",
        "nodemon": "^3.0.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "ts-jest": "^29.1.1",
        "typescript": "^4.1.2",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "joi-to-typescript": "^4.7.0"
    }
}