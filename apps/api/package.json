{
  "name": "api",
  "version": "1.0.0",
  "description": "A Node.js application with Prisma, ES6, Jest, and TypeScript",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "node dist/src/app.js",
    "dev": "nodemon",
    "build": "tsc",
    "test": "jest",
    "prisma:migrate": "prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema src/prisma/schema.prisma"
  },
  "keywords": [
    "node",
    "prisma",
    "jest",
    "typescript",
    "es6"
  ],
  "author": "Bhaskar Sharma",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.2.0",
    "express": "^4.18.2",
    "prisma": "^5.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.2",
    "jest": "^29.6.3",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.2"
  }
}